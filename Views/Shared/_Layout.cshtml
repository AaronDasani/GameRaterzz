<body class="text-danger">
    <nav class="site-header py-4">
        <div class="container">
            <div class="row">
                <a class="p-2 bg-dark text-light rounded text-center col-2" href="~/Home">
                    <i class="fas fa-star-half-alt"> </i> GameRaterzz
                </a>
            
                <form action="~/fetchGameBaseOnSearch" method="post" class=" searchDiv col-6 text-center ml-5">
                    <input class="form-control mr-2 col-7 ml-5" type="text" name="gameName" placeholder="Search games.." >
                    <button class="btn btn-outline-danger my-2 my-sm-0" type="submit">Search</button>
                </form>
                
                <a class="pt-2  d-md-inline-block col-2 text-light ProfileBtn" href="~/UserProfile"><i class="fas fa-user-circle"> </i> <span class="userName">@ViewBag.userName</span></a>
                <a class="py-2 d-md-inline-block btn btn-sm btn-outline-danger text-white col-1" asp-action="logout" asp-controller="LoginAndRegister">Log out</a>
            </div>
        </div>
    </nav>
    @RenderBody()

